      async submitFormChangeStatus () {
        let data = {}
        data.pobj_codigo = this.formChangeStatusData.pobj_codigo
        data.pobj_estado = this.newEstados
        data.aun_codigo_ejecutora = this.dataFormulario.aun_codigo_ejecutora
        data.cac_codigo = this.cacCodigo

        this.dialogButtonLoading = true
        await this.$service.put(
          'SISPOA',
          `/poas-objetivos/estado-all`,
          data
        ).then(async (response) => {
          if (response) {
            this.$toast.clear()
            this.$toast.success(response.err_mensaje)
            this.closeModalDialogChangeStatus()
            this.getAllFormulariosObjetivos()
            this.$nuxt.$emit('actualizadorActividad', true)
            this.$nuxt.$emit('ActualizarFormularioActual',true)
          }
        }).catch(error => {
          this.$toast.error(error.error_mensaje)
        }).finally( () => this.dialogButtonLoading = false )

      },
      
